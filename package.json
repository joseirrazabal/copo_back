{
	"name": "microservice",
	"version": "0.0.1",
	"scripts": {
		"prod:env": "cp .env_example .env",
		"prod:copy": "cp -r .env package.json package-lock.json src/views keys dist",
		"prod:npm": "npm i --only=prod",
		"prod:run": "node server.js",
		"build": "babel src -d dist",
		"build:all": "npm run build && npm run prod:env && npm run prod:copy && cd dist && npm run prod:npm",
		"build:prod": "npm run prod:env && npm run build && npm run prod:copy && cd dist && npm run prod:npm",
		"prod": "npm run build && npm run prod:copy && cd dist && npm run prod:npm && npm run prod:run",
		"lint": "eslint src",
		"prettier": "prettier \"src/**/*.js\" --write",
		"test": "jest",
		"debug": "nodemon --inspect-brk ./src/server.js --exec babel-node -e js",
		"start": "nodemon ./src/server.js --exec babel-node -e js",
		"flow": "flow",
		"precommit": "npm run version",
		"version": "node version.js && git add ."
	},
	"lint-staged": {
		"*.{js,json}": [
			"prettier --write",
			"git add"
		]
	},
	"license": "ISC",
	"dependencies": {
		"@apollo/federation": "^0.20.4",
		"apollo-datasource": "^0.7.2",
		"apollo-server": "^2.18.2",
		"apollo-server-express": "^2.18.2",
		"app-root-dir": "^1.0.2",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"ejs": "^3.1.5",
		"express": "^4.17.1",
		"graphql": "^15.3.0",
		"jest": "^26.5.2",
		"jsonwebtoken": "^8.5.1",
		"kue": "^0.11.6",
		"lodash": "^4.17.20",
		"moment": "^2.29.1",
		"mongodb": "^3.6.2",
		"node-dir": "^0.1.17",
		"promise-mysql": "^4.1.3",
		"redis": "^3.0.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.11.6",
		"@babel/core": "^7.11.6",
		"@babel/node": "^7.10.5",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-proposal-json-strings": "^7.10.4",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-syntax-import-meta": "^7.10.4",
		"@babel/plugin-transform-flow-strip-types": "^7.10.4",
		"@babel/plugin-transform-runtime": "^7.11.5",
		"@babel/preset-env": "^7.11.5",
		"@babel/preset-flow": "^7.10.4",
		"@babel/runtime": "^7.11.2",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.5.2",
		"eslint": "^7.11.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-formatter-pretty": "^4.0.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-flowtype-errors": "^4.3.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-prettier": "^3.1.4",
		"flow-bin": "^0.135.0",
		"flow-typed": "^3.2.1",
		"husky": "^4.3.0",
		"lint-staged": "^10.4.0",
		"nodemon": "^2.0.4",
		"prettier": "^2.1.2"
	},
	"engines": {
		"node": ">=10"
	}
}
